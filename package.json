{
	"name": "vue-klender",
	"version": "0.0.4",
	"license": "Apache-2.0",
	"description": "Yet another date picker component for vue.js",
	"author": {
		"name": "Rizaldi Martaputra",
		"email": "rizaldi.m@gmail.com",
		"url": "https://rizaldim.com"
	},
	"files": [
		"dist/"
	],
	"main": "dist/DatePicker.umd.js",
	"module": "dist/DatePicker.esm.js",
	"unpkg": "dist/DatePicker.min.js",
	"browser": {
		"./sfc": "src/DatePicker.vue"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rizaldim/vue-klender"
	},
	"keywords": [
		"vuejs",
		"date picker"
	],
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/DatePicker.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/DatePicker.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/DatePicker.min.js",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"core-js": "^2.6.5",
		"dayjs": "^1.8.15",
		"lodash": "^4.17.13",
		"vue": "^2.6.10"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.9.2",
		"@vue/cli-plugin-eslint": "^3.9.2",
		"@vue/cli-service": "^3.9.3",
		"babel-eslint": "^10.0.1",
		"eslint": "^5.16.0",
		"eslint-plugin-vue": "^5.0.0",
		"rollup": "^1.17.0",
		"rollup-plugin-buble": "^0.19.8",
		"rollup-plugin-commonjs": "^10.0.1",
		"rollup-plugin-vue": "^5.0.1",
		"sass": "^1.22.6",
		"sass-loader": "^7.1.0",
		"vue-template-compiler": "^2.6.10"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	]
}
